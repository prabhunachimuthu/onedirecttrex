    <script type="text/javascript">
        var charRegex = /\D+/g;
        $(document).ready(function () {
            //hideMask();
            //$(window).bind("beforeunload", function (event) {
            //    return "You have some unsaved changes";
            //});
            $(window).on('beforeunload', function (e) {
                //console.log(e);
                showMask();
                //var confirmationMessage = "\o/";

                //e.returnValue = confirmationMessage;
                //return "Can you log to DB?";
                //alert();
                //$("#divpopup").css("display","block");
                //$("#mask").css("display", "block");
                //alert();
            });
            $(window).on('unload', function () {
                //showMask();

                //alert();
                //$("#divpopup").css("display","block");
                //$("#mask").css("display", "block");
                //alert();
            });

            var pathName = window.location.pathname.match(charRegex);
            var currentLink;
            if (pathName != "/")
                currentLink = $("ul.sidebar-menu a[href*='" + pathName + "']");
            else {
                if ("@Context.Session.GetString("UserType")" != "2") {
                    currentLink = $("ul.sidebar-menu a[href='/Therapist/Index']");
                }
                if ("@Context.Session.GetString("UserType")" == "2") {
                    currentLink = $("ul.sidebar-menu a[href='/Therapist/Profile']");
                }
            }

            if (currentLink) {
                if (currentLink.find("i").hasClass('text-aqua'))
                    currentLink.find("i").removeClass("text-aqua");
                currentLink.find("i").addClass("text-yellow");
            }

        });

        function showMask(isDelay) {

            if (isDelay) {
                console.log("if mask");
                setTimeout(function () {
                    document.getElementById("divMask").style.display = "block";
                    //document.getElementById("divpopup").style.display = "block";
                    //document.getElementById("mask").style.display = "block";
                }, 10);
            }
            else {
                console.log("else mask");
                setTimeout(function () {
                    document.getElementById("divMask").style.display = "block";
                    //document.getElementById("divpopup").style.display = "block";
                    //document.getElementById("mask").style.display = "block";
                }, 10);
            }
        }

        function hideMask() {
            document.getElementById("divMask").style.display = "none";
        }
        function btnSaveClick() {
            alert("button click");
            document.getElementById("divpopup").style.display = "none";
            document.getElementById("mask").style.display = "none";
        }

    </script>

